{
    "compilerOptions": {
      "target": "es2020",
      "module": "es2020",
      "moduleResolution": "node",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true
    },
    "include": ["src"]
  }
  